





import re

def word_count(text):
    word_freq = dict()
    for word in re.split(r'[^\w]', text):
        if len(word) > 1:
            word_freq[word] = word_freq.get(word, 0) + 1
    top10 = sorted(word_freq.items(), key= lambda k_v: k_v[1], reverse=True)[:10]
    return dict(top10)

word_count("aa aa aa bb cc cc A B C a AA")


